<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<style type="text/css">
.lg-3-extend {
	width: 13%
}

.lg-9-extend {
	width: 37%
}

.modal-content {
	width: 800px
}
</style>
<form class="form-horizontal">
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">题目:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="nameID"></p>
		</div>
		<label class="col-lg-3 lg-3-extend control-label">数据库:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="dbTypeID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3  lg-3-extend control-label">编程语言:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="langTypeID"></p>
		</div>
		<label class="col-lg-3  lg-3-extend control-label">价格:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="priceID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">来源:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="sourceTypeID"></p>
		</div>
		<label class="col-lg-3 lg-3-extend control-label">推荐:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="recommendID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">权重:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="weightsID"></p>
		</div>
		<label class="col-lg-3 lg-3-extend control-label">添加时间:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="addTimeID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">添加人:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="adderNameID"></p>
		</div>
		<label class="col-lg-3 lg-3-extend control-label">发布时间:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="postTimeID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">销售量:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="saleNumID"></p>
		</div>
		<label class="col-lg-3 lg-3-extend control-label">更新时间:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="updateTimeID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">更新人:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="updaterNameID"></p>
		</div>
		<label class="col-lg-3 lg-3-extend control-label">浏览量:</label>
		<div class="col-lg-9 lg-9-extend">
			<p class="form-control-static" id="viewNumID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">概要:</label>
		<div class="col-lg-9 ">
			<p class="form-control-static" id="summaryID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 lg-3-extend control-label">详情:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="descriptionID"></p>
		</div>
	</div>
</form>