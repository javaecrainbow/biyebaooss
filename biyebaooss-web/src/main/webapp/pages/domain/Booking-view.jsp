<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<form class="form-horizontal">
	<div class="form-group">
		<label class="col-lg-3 control-label">题目:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="titleID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">数据库:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="dbTypeID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">语言:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="langTypeID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">邮箱:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="mailID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">电话:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="telephoneID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">预定时间:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="bookingTimeID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">备注:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="markID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">需求详情:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="contentsID"></p>
		</div>
	</div>
	<div class="form-group">
		<label class="col-lg-3 control-label">审核人:</label>
		<div class="col-lg-9">
			<p class="form-control-static" id="auditorID"></p>
		</div>
	</div>
</form>